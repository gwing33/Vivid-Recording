<h1>Listing page_infos</h1>

<table>
  <tr>
    <th>Unique</th>
    <th>Name</th>
    <th>Page data</th>
    <th>Type</th>
  </tr>

<% @page_infos.each do |page_info| %>
  <tr>
    <td><%=h page_info.unique_id %></td>
    <td><%=h page_info.name %></td>
    <td><%=h page_info.page_data %></td>
    <td><%=h page_info.type %></td>
    <td><%= link_to 'Show', page_info %></td>
    <td><%= link_to 'Edit', edit_page_info_path(page_info) %></td>
    <td><%= link_to 'Destroy', page_info, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New page_info', new_page_info_path %>